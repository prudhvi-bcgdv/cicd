{
  "name": "poc-node-express-ts",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npx nodemon",
    "format": "prettier --write .",
    "build": "tsc",
    "build:watch": "tsc -w",
    "lint": "eslint src/**/*.ts ",
    "lint:fix": "eslint src/**/*.ts --fix",
    "test": "NODE_ENV=test jest",
    "test:e2e": "NODE_ENV=test jest --testPathPattern='.*.e2e.spec.ts'",
    "test:controller": "NODE_ENV=test jest --testPathPattern='.*.controller.spec.ts'",
    "test:single": "NODE_ENV=test jest -t"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "http-status-codes": "^2.2.0",
    "inversify": "^6.0.1",
    "inversify-express-utils": "^6.4.3",
    "inversify-inject-decorators": "^3.1.0",
    "prettyjson": "^1.2.5",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.3.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.25",
    "@types/yamljs": "^0.2.31",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.0.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "description": "POC using node, express and typescript",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BCGDV/ctd-poc.git"
  },
  "author": "tito olivieri",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/BCGDV/ctd-poc/issues"
  },
  "homepage": "https://github.com/BCGDV/ctd-poc#readme"
}
